{
  "name": "vue-a2ui-demo",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "demo:restaurant": "npm run build:renderer && concurrently -k -n \"AGENT,WEB\" -c \"magenta,blue\" \"npm run serve:agent\" \"npm run dev\"",
    "build:renderer": "cd ../../../renderers/vue && npm install && npm run build",
    "serve:agent": "cd ../../agent/adk/restaurant_finder && uv run ."
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@a2ui/vue": "file:../../../renderers/vue",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "typescript": "^5.7.3",
    "uuid": "^13.0.0",
    "vite": "^6.0.7",
    "vue-tsc": "^2.2.0"
  }
}
